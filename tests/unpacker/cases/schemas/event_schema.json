{
  "version": 3,
  "engine_version": 3,
  "name": "event",
  "tables": {
    "_version": {
      "columns": {
        "version": {
          "data_type": "bigint",
          "nullable": false
        },
        "engine_version": {
          "data_type": "bigint",
          "nullable": false
        },
        "inserted_at": {
          "data_type": "timestamp",
          "nullable": false
        }
      },
      "write_disposition": "append"
    },
    "_loads": {
      "columns": {
        "load_id": {
          "data_type": "text",
          "nullable": false
        },
        "status": {
          "data_type": "bigint",
          "nullable": false
        },
        "inserted_at": {
          "data_type": "timestamp",
          "nullable": false
        }
      },
      "write_disposition": "append"
    },
    "event_slot": {
      "columns": {
        "value": {
          "data_type": "complex",
          "nullable": true
        }
      },
      "write_disposition": "append"
    },
    "event_user": {
      "columns": {},
      "write_disposition": "append",
      "filters": {
        "excludes": [
          "re:^parse_data"
        ],
        "includes": [
          "re:^parse_data__(intent|entities|message_id$|text$)"
        ]
      }
    },
    "event_bot": {
      "columns": {},
      "write_disposition": "append",
      "filters": {
        "excludes": [
          "re:^data",
          "re:^metadata"
        ],
        "includes": [
          "re:^metadata__(utter_action|template_name|rasa_x_[a-z]+)$"
        ]
      }
    }
  },
  "settings": {
    "default_hints": {
      "not_null": [
        "re:^timestamp$",
        "re:^_timestamp$",
        "re:^_dist_key$",
        "re:^_record_hash$",
        "re:^_root_hash$",
        "re:^_load_id$",
        "re:^_parent_hash$",
        "re:^_pos$",
        "re:^sender_id$"
      ],
      "partition": [
        "re:^_timestamp$",
        "re:^timestamp$"
      ],
      "cluster": [
        "re:^_dist_key$",
        "re:^sender_id$"
      ],
      "primary_key": [],
      "foreign_key": [
        "re:^_parent_hash$"
      ],
      "sort": [
        "re:^timestamp$",
        "re:^_timestamp$"
      ],
      "unique": [
        "re:^_record_hash$"
      ]
    },
    "preferred_types": {
      "re:^timestamp$": "timestamp",
      "re:^_timestamp$": "timestamp",
      "re:^inserted_at$": "timestamp",
      "re:confidence": "double",
      "re:^_pos$": "bigint"
    }
  },
  "normalizers": {
    "names": "dlt.common.normalizers.names.snake_case",
    "json": {
      "module": "tests.unpacker.mock_rasa_json_normalizer",
      "config": {
        "propagation": {
          "root": {
            "_record_hash": "_root_hash",
            "timestamp": "_timestamp",
            "sender_id": "_dist_key"
          }
        }
      }
    }
  }
}
