{
  "version": 3,
  "engine_version": 3,
  "name": "event",
  "tables": {
    "_version": {
      "columns": {
        "version": {
          "data_type": "bigint",
          "nullable": false
        },
        "engine_version": {
          "data_type": "bigint",
          "nullable": false
        },
        "inserted_at": {
          "data_type": "timestamp",
          "nullable": false
        }
      },
      "write_disposition": "append"
    },
    "_loads": {
      "columns": {
        "load_id": {
          "data_type": "text",
          "nullable": false
        },
        "status": {
          "data_type": "bigint",
          "nullable": false
        },
        "inserted_at": {
          "data_type": "timestamp",
          "nullable": false
        }
      },
      "write_disposition": "append"
    },
    "event_slot": {
      "columns": {
        "value": {
          "data_type": "complex",
          "nullable": true
        }
      },
      "write_disposition": "append"
    },
    "event_user": {
      "columns": {},
      "write_disposition": "append",
      "filters": {
        "excludes": [
          "^parse_data"
        ],
        "includes": [
          "^parse_data__(intent|entities|message_id$|text$)"
        ]
      }
    },
    "event_bot": {
      "columns": {},
      "write_disposition": "append",
      "filters": {
        "excludes": [
          "^data",
          "^metadata"
        ],
        "includes": [
          "^metadata__(utter_action|template_name|rasa_x_[a-z]+)$"
        ]
      }
    }
  },
  "settings": {
    "default_hints": {
      "not_null": [
        "^timestamp$",
        "^_timestamp$",
        "^_dist_key$",
        "^_record_hash$",
        "^_root_hash$",
        "^_load_id$",
        "^_parent_hash$",
        "^_pos$",
        "^sender_id$"
      ],
      "partition": [
        "^_timestamp$",
        "^timestamp$"
      ],
      "cluster": [
        "^_dist_key$",
        "^sender_id$"
      ],
      "primary_key": [],
      "foreign_key": [
        "^_parent_hash$"
      ],
      "sort": [
        "^timestamp$",
        "^_timestamp$"
      ],
      "unique": [
        "^_record_hash$"
      ]
    },
    "preferred_types": {
      "^timestamp$": "timestamp",
      "^_timestamp$": "timestamp",
      "^inserted_at$": "timestamp",
      "confidence": "double",
      "^_pos$": "bigint"
    }
  },
  "normalizers": {
    "names": "dlt.common.normalizers.names.snake_case",
    "json": {
      "module": "tests.unpacker.mock_rasa_json_normalizer",
      "config": {
        "propagation": {
          "root": {
            "_record_hash": "_root_hash",
            "timestamp": "_timestamp",
            "sender_id": "_dist_key"
          }
        }
      }
    }
  }
}
